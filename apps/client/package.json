{
  "name": "nuxt",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate:types": "graphql-codegen -r dotenv/config --config codegen.ts && mv graphql/exports.d.ts graphql/index.js",
    "generate:schema": "node ./graphql/schemaQuery.mjs",
    "generate": "yarn run generate:types && npm run generate:schema",
    "lint": "eslint",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.0",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@parcel/watcher": "^2.4.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/fonts": "^0.7.0",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/apollo": "5.0.0-alpha.14",
    "@nuxtjs/seo": "2.0.0-rc.9",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@payloadcms/live-preview": "^0.2.2",
    "@pinia/nuxt": "^0.5.1",
    "@types/escape-html": "^1.0.4",
    "@vueuse/core": "^10.10.0",
    "dotenv": "^16.4.5",
    "escape-html": "^1.0.3",
    "graphql-codegen": "^0.4.0",
    "nuxt": "^3.12.1",
    "nuxt-icon": "^0.6.10",
    "nuxt-icon-tw": "^0.1.4",
    "pinia": "^2.1.7",
    "scss": "^0.2.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.3"
  }
}
