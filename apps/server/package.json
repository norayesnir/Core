{
  "name": "payload",
  "private": true,
  "description": "A blank template to get started with Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "resolutions": {
    "sharp": "latest"
  },
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload"
  },
  "dependencies": {
    "@nouance/payload-better-fields-plugin": "^1.4.1",
    "@payloadcms/bundler-vite": "^0.1.7",
    "@payloadcms/plugin-cloud": "^3.0.0",
    "@payloadcms/richtext-lexical": "^0.11.1",
    "copyfiles": "^2.4.1",
    "dashify": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.19.2",
    "payload": "^2.0.0",
    "@payloadcms/db-postgres": "^0.x",
    "payload-blurhash-plugin": "^2.0.2",
    "@payloadcms/plugin-seo": "^2.3.2",
    "payload-webp": "^1.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  }
}
